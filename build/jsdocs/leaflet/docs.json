
{
    "opts": {
        "recurse": true,
        "template": "../template",
        "configPath": "../leaflet",
        "destination": "./docs/leaflet"
    },
    "disqus":"",
    "tags": {
        "allowUnknownTags": true,
        "linkdoc-leaflet":"https://leafletjs.com/reference-1.2.0.html"
    },
    "i18n": {
        "defaultLang": "en",
        "possibleLang": ["en"],
        "dbFilePath":"./build/jsdocs/leaflet"
    },
    "source": {
        "includePattern": ".+\\.js?$",
        "excludePattern": ".+\\\\src\\\\common\\\\iServer\\\\.*Service.js?$",
        "include": [
            "src/common","src/leaflet"
        ],
        "exclude": [
            "src/common/overlay/levelRenderer"
        ]
        
    },
    "plugins": ["plugins/markdown","../plugins/factoryFunction"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": false
        }
        //"applicationName": "iClient for Leaflet"
    }
}
